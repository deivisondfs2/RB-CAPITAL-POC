<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.favorito.model.Favorito" />
	<class name="com.favorito.model.impl.FavoritoImpl" table="favoritos_itens">
		<id name="favoritoId" type="long">
			<generator class="assigned" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="favoritos_itenscol" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>